---
import BaseLayout from '@/layouts/BaseLayout.astro';

const seo = {
  title: 'Land Clearing in Austin, TX | CTX Land Clearing',
  description: 'Land clearing and forestry mulching services for Austin, TX properties.',
  path: '/location/austin-tx/'
};
---
<BaseLayout seo={seo}>
  <section class="section-standard">
    <div class="container-standard max-w-3xl">
      <h1 class="font-[var(--font-display)] text-5xl uppercase">Land Clearing in Austin, TX</h1>
      <p class="mt-3 text-sm text-black/75">CTX Land Clearing and Forestry Mulching serves Austin with lot clearing, forestry mulching, and land grading support for residential and commercial properties.</p>
      <a href="/contact-us/" class="btn-primary mt-5">Get Free Estimate</a>
    </div>
  </section>
</BaseLayout>
